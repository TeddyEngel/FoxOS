#ifndef _CSTDIO
#define _CSTDIO 1

#include <cstdint>

#define EOF (-1)

extern "C"
{
    void outb(uint16_t port, uint8_t value);
    uint8_t inb(uint16_t port);
    uint16_t inw(uint16_t port);
    int printf(const char* __restrict, ...);
    int putchar(int);
    int putint(int);
    int puts(const char*);
}

#endif /* _CSTDIO */